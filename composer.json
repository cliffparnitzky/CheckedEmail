{
	"name":"cliffparnitzky/checked-email",
	"description":"Provides two textfields for forms for double entering a e-mail address and checking for equality. Also adds the option to activate e-mail field with confirmation for members (affects among others the modules `Personal data` and `Registration`).",
	"keywords":["contao", "form", "form field", "e-mail", "e-mail address", "check for equality"],
	"type":"contao-bundle",
	"license":"LGPL-3.0-or-later",
	"authors":[
		{
			"name":"Cliff Parnitzky",
			"email":"contao@cliff-parnitzky.de",
			"role":"Developer"
		}
	],
	"support":{
		"email":"contao@cliff-parnitzky.de",
		"issues":"https://github.com/cliffparnitzky/CheckedEmail/issues",
		"source":"https://github.com/cliffparnitzky/CheckedEmail"
	},
	"require":{
		"php":">=7.4",
		"contao/core-bundle":"~4.13 || ~5.2"
	},
	"autoload": {
		"psr-4": {
			"CliffParnitzky\\FormCheckedEmailBundle\\": "src/"
		},
		"classmap": [
			"src/Resources/contao/"
		],
		"exclude-from-classmap": [
			"src/Resources/contao/config/",
			"src/Resources/contao/dca/",
			"src/Resources/contao/languages/"
		]
	},
	"extra":{
		"contao-manager-plugin": "CliffParnitzky\\FormCheckedEmailBundle\\ContaoManager\\Plugin",
		"branch-alias": {
			"dev-master": "2.1.x-dev"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}